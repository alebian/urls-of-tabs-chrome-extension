{
  "manifest_version": 2,

  "name": "Tabs Saver",
  "description": "This extension saves all the open tabs urls",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Save your tabs!"
  },

  "permissions": [
    "tabs",
    "https://ajax.googleapis.com/"
  ]

}
